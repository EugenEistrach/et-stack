{
  "name": "tanstack-boilerplate",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "vinxi dev",
    "dev:mocks": "cross-env MOCKS=true ENV=test vinxi dev",
    "dev:email": "email dev --port 3002 --dir ./src/email/templates",
    "build": "pnpm build:paraglide && vinxi build",
    "build:paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "start": "cross-env NODE_ENV=production node -r dotenv/config .output/server/index.mjs",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true node -r dotenv/config .output/server/index.mjs",
    "start-with-clean-db": "pnpm db:clear && pnpm start",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./scripts/clear-db.ts",
    "db:seed": "tsx scripts/seed-local.ts",
    "db:reset": "pnpm db:clear && pnpm db:migrate && pnpm db:seed",
    "db:dump": "tsx ./scripts/dump-db.ts",
    "i18n-check": "inlang lint --project ./project.inlang",
    "i18n-auto-translate": "inlang machine translate --project ./project.inlang -f",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e:run": "cross-env CI=true MOCKS=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "verify": "pnpm lint && pnpm typecheck && pnpm test && pnpm test:e2e:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "prettier": "@epic-web/config/prettier",
  "dependencies": {
    "@epic-web/client-hints": "^1.3.5",
    "@hookform/resolvers": "^3.9.1",
    "@inlang/paraglide-js-adapter-vite": "^1.2.40",
    "@mjackson/headers": "^0.8.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.5",
    "@react-email/components": "^0.0.31",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/react-router": "^1.89.0",
    "@tanstack/react-router-with-query": "^1.89.0",
    "@tanstack/react-table": "^8.20.6",
    "@tanstack/start": "^1.89.0",
    "@vitejs/plugin-react": "^4.3.4",
    "better-auth": "1.0.20",
    "better-sqlite3": "^11.7.0",
    "bullmq": "^5.34.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
