  "name": "et-stack",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.2",
  "scripts": {
    "dev": "vinxi dev",
    "dev:mocks": "cross-env MOCKS=true ENV=test vinxi dev",
    "dev:email": "email dev --port 3002 --dir ./src/email/templates",
    "dev:trigger": "bunx trigger.dev@3.3.11 dev",
    "build": "vinxi build",
    "start": "cross-env NODE_ENV=production bun -r dotenv/config .output/server/index.mjs",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true bun -r dotenv/config .output/server/index.mjs",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:generate": "bunx drizzle-kit generate",
    "db:studio": "bunx drizzle-kit studio",
    "db:clear": "bun ./scripts/clear-db.ts",
    "db:seed": "bun scripts/seed-local.ts",
    "db:reset": "bun db:clear && bun db:migrate && bun db:seed",
    "db:dump": "bun ./scripts/dump-db.ts",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e:run": "cross-env CI=true MOCKS=true playwright test --project=e2e",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "test:storybook": "bun run build-storybook && docker run --rm -v $(pwd):/app -v $(pwd)/node_modules:/app/node_modules -w /app -e CI=true mcr.microsoft.com/playwright:v1.50.1 /bin/bash -c \"curl -fsSL https://bun.sh/install | bash && . ~/.bashrc && bunx http-server storybook-static -p 6006 & sleep 2 && PATH=\\\"./node_modules/.bin:$PATH\\\" cross-env TEST_PROJECT=storybook STORYBOOK_URL=http://localhost:6006 playwright test stories.spec.ts --project=storybook\"",
    "test:storybook:update-snapshots": "bun run build-storybook && docker run --rm -v $(pwd):/app -v $(pwd)/node_modules:/app/node_modules -w /app -e CI=true mcr.microsoft.com/playwright:v1.50.1 /bin/bash -c \"curl -fsSL https://bun.sh/install | bash && . ~/.bashrc && bunx http-server storybook-static -p 6006 & sleep 2 && PATH=\\\"./node_modules/.bin:$PATH\\\" cross-env TEST_PROJECT=storybook STORYBOOK_URL=http://localhost:6006 playwright test stories.spec.ts --project=storybook --update-snapshots\"",
    "test:storybook:base": "cross-env TEST_PROJECT=storybook STORYBOOK_URL=http://localhost:6006 playwright test stories.spec.ts --project=storybook",
    "verify": "bun run lint && bun run typecheck && bun run test && bun run test:e2e:run",
    "deploy-trigger": "bunx trigger.dev@3.3.11 deploy --self-hosted --load-image --registry registry.perfux.dev --namespace perfux",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "new:route": "plop route"
  },
