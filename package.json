{
  "name": "tanstack-boilerplate",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "vinxi dev",
    "dev:email": "email dev --port 3002 --dir ./src/email/templates",
    "build": "pnpm build:paraglide && vinxi build",
    "build:paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "start": "node -r dotenv/config .output/server/index.mjs",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./scripts/clear-db.ts",
    "db:reset": "pnpm db:clear && pnpm db:migrate",
    "db:dump": "tsx ./scripts/dump-db.ts",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
