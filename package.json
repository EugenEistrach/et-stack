    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.1",
    "redis-memory-server": "^0.11.0",
    "resend": "^4.0.1",
    "sonner": "^1.7.1",
    "spin-delay": "^2.0.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "valibot": "1.0.0-beta.9",
    "vinxi": "^0.5.1"
